extends layout

block content
  h1= title
  p= des
  if list
    ul
      each val in list
        li
          if val.userName
            a(href='/users/' + val.userName) #{val.userName}
          else if val.headline
            a(href='/posts/' + val.userId + '-' + val.id) #{val.headline} 
            p created at: #{val.createdAt}